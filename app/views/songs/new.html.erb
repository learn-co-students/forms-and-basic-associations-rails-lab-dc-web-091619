<h1>Create a Song</h1>

<%= form_for @song do |f| %>
<label>Title:</label>
<%= f.text_field :title %>
<label>Artist Name:</label>
<%= f.text_field :artist_name %>
<label>Genre:</label>
<%= f.collection_select :genre_name, Genre.all, :name, :name %>
<label>Song Notes:</label>
<input name='song[note_contents][]' id='song_notes_1'>
<input name='song[note_contents][]' id='song_notes_2'>
<input name='song[note_contents][]' id='song_notes_3'>
<%= f.submit %>
<% end %>

